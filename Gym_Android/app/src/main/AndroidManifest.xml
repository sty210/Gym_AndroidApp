<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.baemin.sun.gym_android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".BMI" />
        <activity android:name=".ExerciseExplanation" />
        <activity android:name=".SearchExerciseByPart" />
        <activity android:name=".SearchGymByRegion" />
        <activity android:name=".SearchGym" />
        <activity android:name=".SearchGymDetail" />
        <activity android:name=".Reviews"></activity>
    </application>

</manifest>
